program ValidacaoData;
uses crt;
     var m, a, d: integer;
       mv: integer;


begin
   writeln('Entre com ano');
   read(a);

   writeln('Entre com o mes');
   read(m);

   writeln('Entre com o dia');
   read(d);

   if (m=1) or (m=5) or (m=7) or (m=8) or (m=10) or (m=12) then
   mv:=1
   else
   mv:=0;


end.

